{
   "compilerOptions": {
     "esModuleInterop": true,
     "sourceMap": true,
     "lib": ["ES2020", "DOM"],
     "jsx": "react",
     "module": "esnext",
     "moduleResolution": "Node",
     "target": "es5",
     "strict": true,
     "resolveJsonModule": true,
     "baseUrl": ".",
     "jsxImportSource": "@emotion/react",
     "plugins": [
      {
        "transform": "typescript-loadable-components-plugin"
      }
    ],
     "paths": {
       "@hooks/*": ["hooks/*"],
       "@components/*": ["components/*"],
       "@layouts/*": ["layouts/*"],
       "@pages/*": ["pages/*"],
       "@utils/*": ["utils/*"],
       "@typings/*": ["typings/*"]
     }
   }
 }